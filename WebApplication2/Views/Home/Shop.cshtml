                          <ul class="select">
                            <li>
                              <input class="select_close" type="radio" name="awesomeness" id="awesomeness-close"
                                value="" />
                              <span class="select_label select_label-placeholder">Relevance</span>
                            </li>

                            <li class="select_items">
                              <input class="select_expand" type="radio" name="awesomeness" id="awesomeness-opener" />
                              <label class="select_closeLabel" for="awesomeness-close"></label>

                              <ul class="select_options">

                                <li class="select_option">
                                  <input class="select_input" type="radio" name="awesomeness"
                                    id="awesomeness-ridiculous" />
